<div class="container">
  <div class="row">
    <h1>My Orders</h1>

    <table class="table">
      <thead>
        <tr>
          <th>Pay type</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @orders.each do |order| %>
          <tr>
            <td><%= order.pay_type %></td>
            <td><%= order.line_items.product %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div><!-- end row -->
</div><!-- end container -->